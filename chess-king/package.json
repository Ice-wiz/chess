{
  "name": "chess-king",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev:frontend": "turbo run dev --filter=frontend",
    "dev:ws": "turbo run dev --filter=ws",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "prettier": "^3.5.3",
    "prisma": "^6.6.0",
    "turbo": "^2.5.0",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@prisma/client": "^6.6.0"
  }
}
